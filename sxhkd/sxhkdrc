super + Return
  st

super + v
  xset -display :0 dpms force off

super + space
  rofi -show run

super + Tab
  rofi -show window

XF86AudioMute
  amixer -q sset Master toggle

XF86Audio{Lower,Raise}Volume
  amixer -q sset Master 2%{-,+}

XF86MonBrightness{Up,Down}
  xbacklight -{inc,dec} 3

XF86Sleep
  systemctl suspend
